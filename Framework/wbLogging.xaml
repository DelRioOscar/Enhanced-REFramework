    <Sequence sap2010:Annotation.AnnotationText="#wbType&#xA;If we are not at the root, wbType is the last element when spitting in_wbPath by &quot;|&quot;" DisplayName="Determine wbType" sap2010:WorkflowViewState.IdRef="Sequence_37">
      <Sequence.Variables>
