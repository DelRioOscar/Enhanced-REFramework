    <If sap2010:Annotation.AnnotationText="#wbStatus&#xA;Determine the value of wbStatus depending on inputs io_Exception received and in_wbFinalExec" Condition="[io_Exception is Nothing]" DisplayName="If the log is a successful one, wbStatus depends on weather children wb had errors or not" sap2010:WorkflowViewState.IdRef="If_9">
      <If.Then>
