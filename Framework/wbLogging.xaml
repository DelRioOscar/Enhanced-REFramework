          <Assign.To>
            <OutArgument x:TypeArguments="x:Object">[wbAudit.Item("configError")]</OutArgument>
          </Assign.To>
          <Assign.Value>
            <InArgument x:TypeArguments="x:Object">[True]</InArgument>
          </Assign.Value>
        </Assign>
      </If.Else>
    </If>
    <If sap2010:Annotation.AnnotationText="It is far better to report error but not cause an exception because of a misconfiguered raport than to stop the execution of the program." Condition="[io_ParentAudit is Nothing]" DisplayName="If parent audit is Nothing, create (still config error)" sap2010:WorkflowViewState.IdRef="If_11">
