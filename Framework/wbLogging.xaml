  <Sequence sap2010:Annotation.AnnotationText="The workflow logs the outcome of the execution of a workblock. The in_LogMessage is logged with a severity corresponding to the failiure or success of the workblock.&#xA;&#xA;If no exception is passed, we log &quot;Info&quot;&#xA;If exceptuon exists and is final, we log &quot;Error&quot; and if exception exists and is not final we log &quot;Warn&quot;&#xA;&#xA;The following log fields are added:&#xA;Add Hierarchy Log Fields: &#xA; + wbType - Name of Current Workblock&#xA; + wbParentType - Name of Parent Workblock&#xA; + wbLevel - Level of current workblock (typically level of Parent Workblock + 1)&#xA;&#xA;Add Execution Log Fields: &#xA; + wbStart - the timestamp of the start of execution&#xA; + wbDurationSec - duration of execution in seconds&#xA; + wbDurationHrs - duration of execution in hours&#xA; + wbStatus - If the current workblock is successful and all child workblocks are also successful then set &quot;Successful&quot;, If the current workblock is successful and some child workblocks have failed then set &quot;Finished with Exceptions&quot;, if currect workblock failed then &quot;Failed&quot;&#xA; + wbFinalExec - If the current workblock is successful or is at the last attempt and all parents are at the last attempt, wbFinalExec is true, else it is false&#xA; + wbParentStart - The start time of the parent wb&#xA; + wbPath - Workblock execution path to the current workblock, reflecting all ancestors and self&#xA; + wbKey - md5Hash of wbPath+wbStart" DisplayName="wbLogging" sap2010:WorkflowViewState.IdRef="Sequence_9">
    <Sequence.Variables>
      <Variable x:TypeArguments="scg:Dictionary(x:String, x:Object)" Default="[New system.Collections.Generic.Dictionary(Of String, Object) From &#xA;{ &#xA;{&quot;wbType&quot;, &quot;tbd&quot;},&#xA;{&quot;wbLevel&quot;, -1},&#xA;{&quot;wbParentType&quot;, &quot;tbd&quot;},&#xA;{&quot;wbPath&quot;, in_wbPath},&#xA;{&quot;wbStatus&quot;, &quot;tbd&quot;},&#xA;{&quot;wbExceptionType&quot;, &quot;&quot;},&#xA;{&quot;wbStart&quot;, in_wbStart},&#xA;{&quot;wbParentStart&quot;, in_wbParentStart},&#xA;{&quot;wbFinalExec&quot;, in_wbFinalExec},&#xA;{&quot;wbDurationSec&quot;, 0},&#xA;{&quot;wbDurationHrs&quot;, 0},&#xA;{&quot;message&quot;, in_LogMessage},&#xA;{&quot;level&quot;, &quot;Info&quot;},&#xA;{&quot;wbKey&quot;, &quot;tbd&quot;},&#xA;{&quot;configError&quot;, &quot;tbd&quot;},&#xA;{&quot;optionalInfo&quot;, &quot;tbd&quot;}&#xA;}]" Name="wbAudit" />
    </Sequence.Variables>
    <If sap2010:Annotation.AnnotationText="Test Input data for validity" Condition="[Not(String.IsNullOrEmpty(in_LogMessage)) AndAlso&#xA;Not(String.IsNullOrEmpty(in_wbPath)) AndAlso&#xA;io_ParentAudit IsNot Nothing]" DisplayName="Test all Config inputs" sap2010:WorkflowViewState.IdRef="If_16">
      <If.Then>
        <Assign DisplayName="Valid Config" sap2010:WorkflowViewState.IdRef="Assign_45">
          <Assign.To>
            <OutArgument x:TypeArguments="x:Object">[wbAudit.Item("configError")]</OutArgument>
          </Assign.To>
          <Assign.Value>
